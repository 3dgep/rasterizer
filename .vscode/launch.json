    "configurations": [
        {
            "name": "(gdb) 01-ClearScreen",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/gcc/samples/01-ClearScreen/01-ClearScreen",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/samples",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
